{
  "appId": "com.chatfinacial.app",
  "productName": "ChatFinacial",
  "directories": {
    "output": "release"
  },
  "files": [
    "dist/**/*",
    "dist-electron/**/*",
    "package.json"
  ],
  "asar": true,
  "win": {
    "target": "nsis",
    "signAndEditExecutable": false
  },
  "nsis": {
    "oneClick": false,
    "allowToChangeInstallationDirectory": true
  }
}
